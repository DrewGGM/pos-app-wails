<?xml version="1.0" encoding="utf-8"?><!--
   Backup rules for Kitchen App
   This ensures that user preferences are backed up and restored
   even after uninstalling the app or changing devices
-->
<full-backup-content>
    <!-- Include all SharedPreferences for backup -->
    <include domain="sharedpref" path="kitchen_preferences.xml"/>

    <!-- Include server configuration -->
    <include domain="sharedpref" path="server_config.xml"/>
</full-backup-content>